[32m+[m[32mlet gamseq = [];[m
[32m+[m[32mlet usrseq = [];[m
[32m+[m
[32m+[m[32mlet start = false;[m
[32m+[m[32mlet level = 0;[m
[32m+[m[41m [m
[32m+[m[32mlet h2 = document.querySelector("h2");[m
[32m+[m
[32m+[m[32mlet btns = ["yellow", "red", "green", "blue"];[m
[32m+[m
[32m+[m[32mdocument.addEventListener("keypress", function(){[m
[32m+[m[32m   if(start == false){[m
[32m+[m[32m    console.log("Game is Started");[m
[32m+[m[32m    start = true;[m
[32m+[m
[32m+[m[32m    levelUp();[m
[32m+[m[32m   }[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mfunction btnFlash(btn){[m
[32m+[m[32m    btn.classList.add("flash");[m
[32m+[m[32m    setTimeout(function(){[m
[32m+[m[32m        btn.classList.remove("flash");[m
[32m+[m
[32m+[m[32m    }, 250);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction levelUp(){[m
[32m+[m[32m    usrseq=[];[m
[32m+[m[32m    level ++;[m
[32m+[m[41m    [m
[32m+[m[32m h2.innerText =  `Level ${level}`;[m
[32m+[m
[32m+[m[32m let ranIdx = Math.floor(Math.random() * 3);[m
[32m+[m[32m let ranColor = btns[ranIdx];[m
[32m+[m[32m let ranBtns = document.querySelector(`.${ranColor}`);[m
[32m+[m[32m//  console.log(ranIdx);[m
[32m+[m[32m//  console.log(ranColor);[m
[32m+[m[32m//  console.log(ranBtns);[m
[32m+[m[32mgamseq.push(ranColor);[m
[32m+[m[32mconsole.log(gamseq);[m
[32m+[m[32m btnFlash(ranBtns);[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction userFlash(btn){[m
[32m+[m[32m    btn.classList.add("userFlash");[m
[32m+[m[32m    setTimeout(function(){[m
[32m+[m[32m        btn.classList.remove("userFlash");[m
[32m+[m[32m    },250);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction chackAns(){[m
[32m+[m[32m    let idx = level-1;[m
[32m+[m
[32m+[m[32m    if(usrseq[idx] === gamseq[idx]){[m
[32m+[m[32m       if(usrseq.length == gamseq.length){[m
[32m+[m[32m        setTimeout(levelUp(), 1000);[m
[32m+[m[32m       }[m
[32m+[m[32m    }[m
[32m+[m[32m    else{[m
[32m+[m[32m    h2.innerHTML = `Game Over ! Your Score was <b> ${level} </b> <br> Try again TO press any Key `;[m[41m  [m
[32m+[m[32m     document.querySelector("body").style.backgroundColor = "red";[m
[32m+[m[32m     setInterval(function(){[m
[32m+[m[32m        document.querySelector("body").style.backgroundColor = "white";[m
[32m+[m
[32m+[m[32m     }, 150);[m[41m  [m
[32m+[m[32m    reset();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32mfunction btnPress(){[m
[32m+[m[32m    // console.log("BUTTON CLICKED IS ", this);[m
[32m+[m[32m    let btn = this;[m
[32m+[m[32m    userFlash(btn);[m
[32m+[m
[32m+[m[32m    userColor = btn.getAttribute("id");[m
[32m+[m[32m    usrseq.push(userColor);[m
[32m+[m
[32m+[m[32m    chackAns(usrseq.length-1);[m
[32m+[m[32m}34[m[41m                                                                                                                                                                                                                                 [m
[32m+[m
[32m+[m[32mlet allbtn = document.querySelectorAll(".btn");[m
[32m+[m
[32m+[m[32mfor(btn of allbtn){[m
[32m+[m[32m    btn.addEventListener("click", btnPress);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction reset(){[m
[32m+[m[32m  start = false;[m
[32m+[m[32m  gamseq = [];[m
[32m+[m[32m  usrseq = [];[m
[32m+[m[32m  level = 0;[m
[32m+[m[32m}[m
