[1m+++ b/CountDown/script.js[m
[36m@@ -0,0 +1,41 @@[m
[32m+[m[32mconst endDate = new Date("1 Sep , 2025 20:00:00").getTime();[m
[32m+[m[32mconst strtDate = new Date().getTime();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mlet x = setInterval(function updateTimer(){[m
[32m+[m
[32m+[m[32m    const currDate =  new Date().getTime();[m
[32m+[m[41m     [m
[32m+[m[32m    const distCover =  currDate-strtDate;[m
[32m+[m
[32m+[m[32m    const distPending = endDate - currDate;[m
[32m+[m
[32m+[m[32m    const days = Math.floor((distPending / (24*60*60*1000)));[m
[32m+[m[32m    const hrs = Math.floor((distPending%(24*60*60*1000))/ (60*60*1000) );[m
[32m+[m[32m    const min = Math.floor((distPending % (60 * 60 * 1000))/(60*1000));[m
[32m+[m[32m    const secs = Math.floor((distPending %( 60 *1000)) / (1000))[m
[32m+[m
[32m+[m
[32m+[m[32m    document.querySelector("#days").innerHTML = days;[m
[32m+[m[32m    document.querySelector("#hours").innerHTML = hrs;[m
[32m+[m[32m    document.querySelector("#minutes").innerHTML = min;[m
[32m+[m[32m    document.querySelector("#seconds").innerHTML = secs;[m
[32m+[m
[32m+[m
[32m+[m[32m    const totalDist = endDate - strtDate;[m
[32m+[m
[32m+[m[32m    const percentageDistance = (distCover/totalDist)*100;[m
[32m+[m
[32m+[m[32m    document.getElementsByClassName("progress-bar").style.width = percentageDistance + "%";[m
[32m+[m
[32m+[m
[32m+[m[32m    if(distPending < 0){[m
[32m+[m[32m        clearInterval(x);[m
[32m+[m[32m        document.getElementsByClassName("box-contener").innerHTML = "EXPIRED";[m
[32m+[m[32m        document.getElementsByClassName("progress-bar").style.width  = "100%";[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}, 1000);[m
