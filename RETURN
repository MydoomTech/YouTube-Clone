[32m+[m[32masync function searchAI() {[m
[32m+[m[32m  const query = document.getElementById("searchInput").value;[m
[32m+[m[32m  const resultsDiv = document.getElementById("results");[m
[32m+[m
[32m+[m[32m  if (!query) {[m
[32m+[m[32m    resultsDiv.innerHTML = "Please enter a query.";[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  resultsDiv.innerHTML = "‚è≥ Searching...";[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    const response = await fetch("https://api.openai.com/v1/chat/completions", {[m
[32m+[m[32m      method: "POST",[m
[32m+[m[32m      headers: {[m
[32m+[m[32m        "Content-Type": "application/json",[m
[32m+[m[32m        "Authorization": "thread_EIpHrTAVe0OzoLQg3TXfvrkG"  // üîë put your key here[m
[32m+[m[32m      },[m
[32m+[m[32m      body: JSON.stringify({[m
[32m+[m[32m        model: "gpt-3.5-turbo",[m
[32m+[m[32m        messages: [{ role: "user", content: query }][m
[32m+[m[32m      })[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    const data = await response.json();[m
[32m+[m[32m    resultsDiv.innerHTML = data.choices[0].message.content;[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    resultsDiv.innerHTML = "‚ùå Error: " + error.message;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
